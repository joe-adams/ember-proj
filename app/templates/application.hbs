    <div class="jumbotron">
      <div class="container">
        <h1>Pick a Stock by Name</h1>
        {{auto-complete data=model}}
      </div>
    </div>
     <div class='container-fluid'>
    	<div class='row'>
    		<div class="col-md-4 col-md-offset-1 table-responsive">
			    {{#if model.resultSet.stocks.length}}
			    	<table class='table-striped'>
				    	<tr><th>Name</th><th>Symbol</th></tr>
				    	<tbody>
						    {{#each  model.resultSet.stocks as |stock|}}
						    	<tr {{action 'chart' stock.id}}><td>{{stock.name}}</td><td>{{stock.symbol}}</td></tr>
						    {{/each}}
				    	</tbody>	
					</table>
			    {{else}}
			    	<table class='table'>
			    		<tr><td>There are no matching stocks</td></tr>
			    	</table>
			    {{/if}}
		    </div>
		<div class="col-md-4 col-md-offset-1 table-responsive" id='chartContainer'>

			{{outlet}}
		</div>
	</div>

    	
    </div>
